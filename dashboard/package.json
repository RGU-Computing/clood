    "angular": "^1.7.5",
    "angular-animate": "^1.7.9",
    "angular-loader": "^1.7.5",
    "angular-resource": "^1.7.5",
    "angular-route": "^1.7.5",
    "angularjs-toaster": "^3.0.0",
    "bootstrap": "^3.4.1",
    "html5-boilerplate": "0.0.1",
    "jquery": "^3.4.1",
    "papaparse": "^5.1.1"
  },
  "devDependencies": {
    "angular-mocks": "^1.7.5",
